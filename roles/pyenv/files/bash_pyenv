## pyenv

if [[ -d ~/.pyenv ]]; then
  export PYENV_ROOT=~/.pyenv
  export PATH=$(pathcat "${PYENV_ROOT}/bin" "${PATH}")
  if type pyenv &>/dev/null; then
    if [[ -z "${PYENV_LOADING}" ]]; then
      export PYENV_LOADING=1
      eval "$(pyenv init -)"
      eval "$(pyenv virtualenv-init -)"
      unset PYENV_LOADING
    fi
  fi
fi
